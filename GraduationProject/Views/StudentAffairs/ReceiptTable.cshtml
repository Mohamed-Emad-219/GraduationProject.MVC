@{
	ViewData["Title"] = "Receipts";
}

<section class="receipts">
    <div class="container py-5">
        <h1 class="fs-3 mb-4 text-center">Receipts</h1>
        <div class="w-60 m-auto">
            <table class="table table-hover table-striped" id="receipts">
                <thead class="table-secondary">
                    <tr>
                        <th>#</th>
                        <th>Student ID</th>
                        <th>Name</th>
                        <th>Payment</th>
                        <th>Status</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</section>

