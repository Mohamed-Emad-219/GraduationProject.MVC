@{
	ViewData["Title"] = "Statistics";
}

<section class="financial-manager">
    <div class="container py-3">
        <h2 class="text-center">Statistics</h2>
        <h4 class="text-center">College: Computer Science & Artificial Intillegence</h4>
        <p class="text-center fs-4">Students of Fall 2024</p>
        <div class="table-responsive w-75 mx-auto">
            <table class="table table-hover" id="std-stats">
                <thead class="table-secondary">
                    <tr>
                        <th>#</th>
                        <th>Name</th>
                        <th>Class</th>
                        <th>CGPA</th>
                    </tr>
                </thead>
            </table>
        </div>

        <div class="text-center mt-2">
            <button class="btn btn-primary btn-lg m-auto" onclick="window.print()">Print</button>
        </div>
    </div>
</section>

@section Scripts {
        <script src="~/js/studentaffairs.js"></script>
}